{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ES2022",
    "target": "ES2022",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "rootDir": "./",
    "outDir": "./dist"
  },
  "include": ["**/*.ts", "**/*.d.ts"]
}
